{
  "compilerOptions": {
  "declaration": true,
  "esModuleInterop": true,
  "resolveJsonModule": true,
  "strictNullChecks": true,
  "moduleResolution": "node",
  "declarationDir": "dts",
  "outDir": "esm",
  "importsNotUsedAsValues": "error",
  "experimentalDecorators": true,
  "noImplicitAny": false,
  "allowJs": true,
  "skipLibCheck": true,
  "importHelpers": true,
  "strictPropertyInitialization": false,
  "target": "es2015",
  "module": "ESNext",
  "types": [
      "webpack-env",
      "web-ie11"
  ],
  "lib": [
      "DOM",
      "es2017",
      "ES2016.Array.Include"
  ]
},
"exclude": ["examples/src", "rollup.config.ts", "dist", "utils"],
"include": ["packages", "extendPackages"]
// "": []
}